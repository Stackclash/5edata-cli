{
  "name": "5etools-markdown",
  "type": "module",
  "version": "0.0.0",
  "description": "NodeJS cli tool to convert 5etools JSON to markdown files",
  "main": "dist/index.js",
  "bin": {
    "5edata-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "npm run build && npm start",
    "build": "tsc",
    "build:standalone": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/cli.js --external:fsevents",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stackclash/5etools-markdown.git"
  },
  "keywords": [
    "ttrpg",
    "5etools",
    "markdown"
  ],
  "author": "Richard Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stackclash/5etools-markdown/issues"
  },
  "homepage": "https://github.com/Stackclash/5etools-markdown#readme",
  "dependencies": {
    "commander": "^14.0.1",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.5.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
