{
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "@oclif/prettier-config"],
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "tabWidth": 2,
  "tabs": false,
  "endOfLine": "auto",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "",
    "^[..]",
    "^[.]"
  ],
  "importOrderTypeScriptVersion": "5.1.3",
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}